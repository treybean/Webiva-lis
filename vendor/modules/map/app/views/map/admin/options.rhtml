<% if MapZipcode.count(:all) == 0 %>
<% action_panel do |p| -%>
  <%= link_to "Create Zipcodes",:action => 'import_zipcodes',:icon => 'add.gif' %>
<% end -%>
<hr/>
<% end -%>
<div class='admin_content'>
 
<% cms_form_for :options, @options, :html => { :class => 'admin_form' } do |f| -%>
  <%= f.text_field :api_key, :description => 'Google Maps API Key', :required => true %>
  <%= f.text_field :locations_name %>
  <%= f.submit_tag 'Update' %>
<% end -%>
</div>
